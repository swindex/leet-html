{
	"name": "leet-html",
	"displayName": "leet-html",
	"description": "Syntax highlighting and IntelliSense for html inside of JavaScript and TypeScript literal template strings",
	"version": "1.1.1",
	"publisher": "EldarGerfanov",
	"icon": "docs/logo.png",
	"galleryBanner": {
		"color": "#fcfcfc",
		"theme": "light"
	},
	"repository": {
		"url": "https://github.com/swindex/leet-html"
	},
	"bugs": {
		"url": ""
	},
	"engines": {
		"vscode": "^1.15.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"javascript",
		"typescript",
		"html",
		"literal",
		"template"
	],
	"contributes": {
    "grammars": [
			{
				"injectTo": [
					"source.html",
					"source.js",
					"source.js.jsx",
					"source.jsx",
					"source.ts",
					"source.tsx",
					"text.html"
				],
				"scopeName": "inline.leet-html",
				"path": "./syntaxes/leet-html.json",
				"embeddedLanguages": {
					"meta.embedded.block.html": "html"
				}
			}
		]
	},
	"__metadata": {
		"id": "",
		"publisherId": "",
		"publisherDisplayName": "Eldar Gerfanov"
	}
}